{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^db:generate"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "format": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^db:generate"
      ]
    },
    "db:generate": {
      "inputs": [
        "packages/db/prisma/schema.prisma"
      ]
    }
  }
}